    "def url_ex(dfrow):\n",
    "    url = re.findall(\"(http.*)\\s\",dfrow['body'])\n",
    "    if len(url) > 0:\n",
    "        dfrow['url'] = url[0]\n",
    "    else:\n",
    "        dfrow['url'] = None\n",
    "    return dfrow"
   ]
  },
